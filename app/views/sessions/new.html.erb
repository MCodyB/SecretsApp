<h1>Log In</h1>
<div class="form-fields inner-box">
	<%= form_for :session, :url => session_url do |f| %>
		<p class="form-input"><%= f.label :username %>
		<%= f.text_field :username %></p>
		<p class="form-input"><%= f.label :password %>
		<%= f.password_field :password %></p>
		<p class="form-button"><%= f.submit "Sign In" %></p>
	<% end %>
</div>